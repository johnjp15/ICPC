			int i;
			for (i = 0; i < n; i++) {
				t -= sc.nextInt();
				if (t < 0 ) {
					break;
				}
			}
			String s = sc.next();
			
			String repeated = "";
			for (char ch : s.toCharArray()) {
				for (int i = 0; i < r; i++) {
					repeated += ch;
				}
			}
			
			System.out.println(n + " " + repeated);
		}
	}

