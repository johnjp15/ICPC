import java.util.Scanner;

public class ProblemC	{

	public static void main(String[] args)
	{
		Scanner sc = new Scanner(System.in);
		
		int p = sc.nextInt();
		
		while(sc.hasNext())
		{
			int n = sc.nextInt();
			double d = sc.nextDouble();
			
			String bytes = "";
			
			if (d < 0) {
				bytes += "1";
				d *= -1;
			} else {
				bytes += "0";
			}
			
//			for(){
//				
//			}
			Double.doubleToRawLongBits(0.5);
		}
	}

}
